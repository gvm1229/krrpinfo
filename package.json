{
  "name": "krrpinfo",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Hojin (Tom) Jeong",
    "email": "hojini1229@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gvm1229/krrpinfo"
  },
  "engines": {
    "node": ">=20.x",
    "yarn": ">=4.1.1",
    "npm": "yarn only"
  },
  "scripts": {
    "dev": "concurrently \"contentlayer dev\" \"next dev\" \"yarn open-browser\" \"yarn storybook\"",
    "open-browser": "start http://localhost:3000",
    "build": "contentlayer build && next build",
    "analyze": "ANALYZE=true next build",
    "turbo": "next dev --turbo",
    "start": "next start",
    "preview": "next build && next start",
    "storybook": "storybook dev --quiet -p 6006",
    "build-storybook": "storybook build --test",
    "generate-stories": "node scripts/generate-stories.js",
    "lint": "next lint",
    "lint-fix": "next lint --fix",
    "clean": "rm -rf dist && rm -rf .husky/_",
    "clean-all": "yarn clean && rm -rf node_modules && yarn cache clean --all",
    "refresh": "yarn clean-all && yarn install --immutable",
    "reinit": "yarn clean-all && rm yarn.lock && yarn install",
    "upgrade-deps": "yarn ncu -u && yarn install",
    "git-refresh": "git lfs update --force && git rm -rf --cached . && git add .",
    "postinstall": "husky",
    "shadcn": "npx shadcn-ui@latest add",
    "commit": "git cz"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^14.1.3",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@tailwindcss/typography": "^0.5.10",
    "@upstash/redis": "^1.28.4",
    "@vercel/analytics": "^1.2.2",
    "@vercel/speed-insights": "^1.0.10",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "contentlayer": "^0.3.4",
    "date-fns": "^3.4.0",
    "embla-carousel-autoplay": "^8.0.0",
    "embla-carousel-react": "^8.0.0",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.356.0",
    "next": "14.1.3",
    "next-contentlayer": "^0.3.4",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18.2.0",
    "react-scroll": "^1.9.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/addon-mdx-gfm": "8.0.0",
    "@storybook/blocks": "^8.0.0",
    "@storybook/nextjs": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@storybook/test": "^8.0.0",
    "@types/date-fns": "^2.6.0",
    "@types/node": "^20.11.26",
    "@types/react": "^18.2.65",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "autoprefixer": "^10.4.18",
    "commitizen": "^4.3.0",
    "commitlint-config-gitmoji": "^2.3.1",
    "concurrently": "^8.2.2",
    "cz-emoji-conventional": "^1.0.2",
    "dotenv": "^16.4.5",
    "eslint": "^8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "14.1.3",
    "eslint-plugin-autofix": "^1.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-tailwindcss": "^3.14.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "mdast-util-toc": "^7.0.0",
    "npm-check-updates": "^16.14.15",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
    "react-markdown": "^9.0.1",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark": "^15.0.1",
    "remark-gfm": "3.0.1",
    "storybook": "^8.0.0",
    "typescript": "^5.4.2",
    "typescript-eslint": "^7.2.0",
    "unist-util-visit": "^5.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji-conventional"
    }
  },
  "overrides": {
    "next-contentlayer": {
      "next": "$next"
    }
  },
  "packageManager": "yarn@4.1.1"
}
