  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint_fix": "next lint --fix",
    "storybook": "storybook dev -p 6060",
    "clean": "rm -rf dist",
    "deploy": "yarn build && netlify deploy --prod --dir dist --message \"Version 0.2 - Deploy message\"",
    "import_env": "netlify env:import .env",
    "switch_creds": "netlify unlink && netlify logout && netlify login && netlify link",
    "reinit": "rm -rf node_modules && rm yarn.lock && yarn cache clean && yarn install",
    "upgrade_reinit": "yarn ncu -u && yarn reinit"
  },
  "dependencies": {
    "next": "14.1.0",
    "npm-check-updates": "^16.14.12",
    "react": "^18.2.0",
    "react-dom": "^18",
    "storybook": "^7.6.10"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/react": "^7.6.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.17",
    "eslint": "^8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-autofix": "^1.1.0",
    "eslint-plugin-import": "^2.29.1",
