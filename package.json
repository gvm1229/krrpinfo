{
  "name": "krrpinfo",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Megi",
    "email": "hojini1229@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gvm1229/krrpinfo"
  },
  "engines": {
    "node": ">=20.11.0",
    "yarn": ">=4.1.0",
    "npm": "yarn only"
  },
  "scripts": {
    "dev": "concurrently \"next dev\" \"yarn open-browser\" \"yarn storybook\" \"yarn watch-stories\"",
    "open-browser": "start http://localhost:3000",
    "build": "next build",
    "turbo": "next dev --turbo",
    "start": "next start",
    "preview": "next build && next start",
    "storybook": "storybook dev --quiet -p 6006",
    "build-storybook": "storybook build --test",
    "generate-stories": "node scripts/generate-stories.js",
    "watch-stories": "node scripts/watch-stories.js",
    "lint": "next lint",
    "lint-fix": "next lint --fix",
    "clean": "rm -rf dist && rm -rf .next && rm -rf .husky/_",
    "clean-all": "yarn clean && rm -rf node_modules && yarn cache clean --all",
    "refresh": "yarn clean-all && yarn install --immutable",
    "reinit": "yarn clean-all && yarn install",
    "upgrade-deps": "yarn ncu -u && yarn install",
    "git-refresh": "git lfs update --force && git rm -r -f --cached . && git add . && git reset",
    "postinstall": "husky",
    "shadcn": "npx shadcn-ui@latest add"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@next/mdx": "^14.1.0",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@tailwindcss/typography": "^0.5.10",
    "@vercel/analytics": "^1.2.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.335.0",
    "markdown-to-jsx": "^7.4.1",
    "mongodb": "^6.3.0",
    "next": "14.1.0",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18.2.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "remark-toc": "^9.0.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/addon-onboarding": "1.0.11",
    "@storybook/blocks": "^7.6.17",
    "@storybook/nextjs": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/test": "^7.6.17",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "eslint": "^8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-autofix": "^1.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-tailwindcss": "^3.14.2",
    "husky": "9.0.1",
    "lint-staged": "^15.2.2",
    "npm-check-updates": "^16.14.15",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
    "storybook": "^7.6.17"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "yarn lint-fix"
  },
  "packageManager": "yarn@4.1.0"
}
