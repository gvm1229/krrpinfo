{
  "name": "krrpinfo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint-fix": "next lint --fix",
    "storybook": "storybook dev -p 6060",
    "clean": "rm -rf dist",
    "deploy": "yarn build && netlify deploy --prod --dir dist --message \"Version 0.2 - Deploy message\"",
    "import-env": "netlify env:import .env",
    "switch-creds": "netlify unlink && netlify logout && netlify login && netlify link",
    "refresh": "rm -rf node_modules && yarn cache clean --all && yarn install --immutable",
    "reinit": "rm -rf node_modules && rm yarn.lock && yarn cache clean --all && yarn install",
    "upgrade-deps": "yarn ncu -u && yarn install",
    "git-refresh": "git lfs update --force && git rm -r --cached . && git add .",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "mongodb": "^6.3.0",
    "next": "14.1.0",
    "react": "^18.3.0-canary-247738465-20240130",
    "react-dom": "^18.3.0-canary-247738465-20240130"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "daisyui": "^4.6.0",
    "dotenv": "^16.4.1",
    "eslint": "^8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-autofix": "^1.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-tailwindcss": "^3.14.1",
    "husky": "^9.0.6",
    "lint-staged": "^15.2.0",
    "npm-check-updates": "^16.14.14",
    "postcss": "^8.4.33",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
